#%RAML 1.0 DataType

type: object
properties:
  ID:
    type: string
    required: true 
  titre:
    type:  string
    required: true
  type:
    required: true
    enum:
      - fourniture
      - mobilier
      - appareil
  stock_available: 
    type: integer
    required: true
  date_update: 
    type: datetime
    default: 1900-01-01T00:00:00Z
    required : false
     

    


